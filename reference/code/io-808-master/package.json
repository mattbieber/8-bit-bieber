    "test": "true",
    "fmt": "prettier --write 'src/**/*.js'",
    "start": "webpack-serve ./webpack.config.dev.js",
    "tutorial-dev": "./tutorialDev.js",
    "deploy": "./deploy-ghpages.sh"
